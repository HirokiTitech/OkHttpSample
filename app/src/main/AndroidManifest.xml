<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lyricaloriginal.okhttpsample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GetSampleActivity"
            android:exported="true"
            android:label="@string/get_sample" />
        <activity
            android:name=".PostJsonSampleActivity"
            android:exported="true"
            android:label="@string/post_json_sample_sample" />
        <activity android:name=".PostFileSampleActivity" />
        <activity
            android:name=".AuthenticateActivity"
            android:exported="true"
            android:label="@string/auth_sample" />
        <activity
            android:name=".LoggingSampleActivity"
            android:exported="true"
            android:label="@string/logging_sample" />
        <activity
            android:name=".DiskCacheActivity"
            android:exported="true"
            android:label="@string/disk_cache_sample"></activity>
    </application>

</manifest>
